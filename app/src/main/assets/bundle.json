{
  "version": "1.0.0",
  "bundles": [
    {
      "uri": "lib.style",
      "pkg": "com.sunfusheng.small.lib.style"
    },
    {
      "uri": "lib.framework",
      "pkg": "com.sunfusheng.small.lib.framework"
    },
    {
      "uri": "main",
      "pkg": "com.sunfusheng.small.app.main"
    },
    {
      "uri": "phone",
      "pkg": "com.sunfusheng.small.app.phone",
      "rules": {
        "beijing": ".ui.BeijingPhone",
        "weihai": ".ui.WeihaiPhone"
      }
    },
    {
      "uri": "weather_beijing",
      "pkg": "com.sunfusheng.small.app.weather",
      "rules": {
        "beijing": ".ui.BeijingWeather"
      }
    },
    {
      "uri": "weather_shanghai",
      "pkg": "com.sunfusheng.small.app.shanghai.weather",
      "rules": {
        "shanghai": ".ui.ShanghaiWeather"
      }
    },
    {
      "uri": "about",
      "pkg": "com.sunfusheng.small.app.about"
    }
  ]
}